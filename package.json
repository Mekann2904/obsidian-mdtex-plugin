{
	"name": "mdtex-plugin",
	"version": "1.1.1",
	"description": "MdTex is a tool that converts Markdown to PDF using Pandoc and LuaLaTeX. It is characterized by its stable support for Japanese output.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Mekann",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^8.24.1",
		"@typescript-eslint/parser": "^8.24.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.57.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"codemirror": "^5.65.18",
		"glob": "^11.0.1",
		"lodash": "^4.17.21",
		"rimraf": "^6.0.1"
	}
}
