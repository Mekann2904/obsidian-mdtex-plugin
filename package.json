{
  "name": "mdtex-plugin",
  "version": "1.1.1",
  "description": "MdTex is a tool that converts Markdown to PDF using Pandoc and LuaLaTeX. It is characterized by its stable support for Japanese output.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepare": "husky",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Mekann",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/language": "^6.9.2",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitest/coverage-v8": "^4.0.15",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "markdownlint-cli2": "^0.18.1",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "codemirror": "^5.65.18",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "rimraf": "^6.0.1"
  }
}
